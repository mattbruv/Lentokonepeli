{
  "name": "lentokonepeli",
  "version": "1.0.0",
  "description": "A modern remake of Lentokonepeli/Dogfight",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint **/*.ts",
    "collision": "parcel client/collision/index.html",
    "build": "parcel build client/public/index.html",
    "build-no-server": "env MODE=client parcel build client/public/index.html",
    "server": "ts-node server/server.ts",
    "no-server": "env MODE=client parcel client/public/index.html",
    "watch-client": "parcel watch client/public/index.html",
    "clean": "rm -rf .cache && rm -rf dist",
    "assets": "cp -r ./client/public/images ./dist/images",
    "setup": "npm run clean && mkdir dist && npm run assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattbruv/Lentokonepeli.git"
  },
  "keywords": [
    "lentokonepeli",
    "dogfight",
    "remake",
    "aapeli",
    "playray",
    "playforia"
  ],
  "author": "Matt P.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mattbruv/Lentokonepeli/issues"
  },
  "homepage": "https://github.com/mattbruv/Lentokonepeli#readme",
  "dependencies": {
    "@types/express": "^4.17.2",
    "@types/js-cookie": "^2.2.5",
    "@types/ws": "^7.2.2",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "parcel": "^1.12.4",
    "pixi.js": "^5.0.4",
    "prettier": "^1.18.2",
    "rect": "^1.2.1",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "sass": "^1.22.5",
    "ts-jest": "^24.0.2"
  }
}
